config { 
  type: "view"
  }

WITH xvendaparcela AS (
	SELECT 	
		* 
	FROM 
		landing.T_VW_XVENDAPARCELA
)

SELECT

CAST(IDLAN AS INT) AS ID_LAN,
CAST(CODCOLIGADA AS INT) AS ID_COLIGADA,
CAST(NUMVENDA AS INT) AS ID_VENDA,
CAST(NUMADITCANCEL AS INT) AS ID_ADIT_CANCELA,
CAST(NUMPARC AS INT) AS NUM_PARCELA,
CAST(DATA_CARGA AS DATETIME) AS DAT_ALTERACAO,
CAST(TIME_STAMP AS TIMESTAMP)     AS DAT_CARGA

FROM 
xvendaparcela